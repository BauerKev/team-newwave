<!DOCTYPE html>
<html>
<head>
  <title>Workspace</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<nav class="navbar navbar-default">
<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
  <span class="sr-only">Toggle navigation</span>
  <span class="icon-bar"></span>
  <span class="icon-bar"></span>
  <span class="icon-bar"></span>
  </button>
 <div class="container-fluid collapse navbar-collapse" id="bs-example-navbar-collapse-1">
    <div class="navbar-header">
      <a class="navbar-brand" href="#">Coog's Wall</a>
    </div>
    <ul class="nav navbar-nav">
      <li><a href="#">Coog's Post</a></li>
      <li><a href="#">Social Media</a></li>
      <li><a>|</a></li> 
    </ul>
    <form class="navbar-form navbar-right">
        <button type="button" class="btn btn-default btn-md"><%= link_to 'New Post', new_post_path %></button>
    </form>
  </div>
</nav>
<%= yield %>

</body>
</html>
